pkgname=python-logilab-astng
_F_archive_name=logilab-astng
pkgver=0.24.0
pkgrel=1
pkgdesc="Python Abstract Syntax Tree New Generation."
url="http://www.logilab.org/project/logilab-astng"
archs=('i686' 'x86_64')
depends=('python-logilab-common>=0.58.1')
groups=('devel-extra')
up2date="Flasttar http://download.logilab.org/pub/astng/"
source="http://download.logilab.org/pub/astng/$_F_archive_name-$pkgver.tar.gz"
Finclude python
_F_cd_path="$_F_archive_name-$pkgver"
sha1sums=('7883e6b87ae9defa33feb469924fcd9943214036')

build()
{
	Fbuild
	# To resolve conflict between logilab common and astng.
	# The file __init__.py generated by both common and astng
	# is exactly the same, so we delete it.
	Frm $_F_python_libdir/logilab/__init__.py
}
