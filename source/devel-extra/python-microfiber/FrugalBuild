pkgrel=1
pkgdesc="A fabric for a lightweight Couch."
depends=('python-3.0>=3.2.3')
makedepends=('python-sphinx')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_launchpad_dirname="microfiber"
_F_launchpad_name="microfiber"
_F_launchpad_subdir="12.07"
Finclude launchpad python
